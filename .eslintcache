{\n          eventID,\n          name: teamname!,\n          members,\n        },\n      },\n    }).catch((err) => console.log(err.message));\n  };\n\n  if (loading || loading2) {\n    onClose = () => {\n      window.location.reload();\n    };\n    return (\n      <Modal isOpen={true} onClose={onClose}>\n        <ModalOverlay />\n        <ModalContent backgroundColor=\"#e2e19c\" color=\"black\">\n          <ModalHeader>Loading...</ModalHeader>\n          <ModalCloseButton />\n        </ModalContent>\n      </Modal>\n    );\n  }\n\n  if (data1?.register.registered || data2) {\n    onClose = () => {\n      window.location.reload();\n    };\n    return (\n      <Modal isOpen={true} onClose={onClose}>\n        <ModalOverlay />\n        <ModalContent backgroundColor=\"#addfd0\" color=\"black\">\n          <ModalHeader>Registrated Successfully</ModalHeader>\n          <ModalCloseButton />\n        </ModalContent>\n      </Modal>\n    );\n  }\n\n  if (data1?.register.eventPay) {\n    return <PayRegister data={data1.register.eventPay} />;\n  }\n\n  if (error || error2) {\n    error\n      ? (onClose = () => {\n          if (error.message === \"Please login to continue\") {\n            history.push(\"/login\");\n          }\n          window.location.reload();\n        })\n      : (onClose = () => {\n          window.location.reload();\n        });\n    return (\n      <Modal isOpen={true} onClose={onClose}>\n        <ModalOverlay />\n        <ModalContent backgroundColor=\"#f1aaaa\" color=\"black\">\n          <ModalHeader>{error ? error.message : error2?.message}</ModalHeader>\n          <ModalCloseButton />\n        </ModalContent>\n      </Modal>\n    );\n  }\n\n  return (\n    <div>\n      {!isAdmin ? (\n        data.registrationType === \"NONE\" ? (\n          <Box marginTop=\"2vh\" height=\"1vw\">\n            <Alert status=\"info\" size={\"xs\"}>\n              <AlertIcon />\n              Registration is not required for this event\n            </Alert>\n          </Box>\n        ) : (\n          <Box marginTop=\"2vh\" height=\"1vw\">\n            <Button\n              backgroundColor={buttoncolor}\n              color=\"black\"\n              onClick={\n                data.registrationType === \"INDIVIDUAL\"\n                  ? () => {\n                      IndividualReg(data.id);\n                    }\n                  : onOpen\n              }\n            >\n              REGISTER NOW\n            </Button>\n          </Box>\n        )\n      ) : null}\n      <Modal isOpen={isOpen} onClose={onClose}>\n        <ModalOverlay />\n        <ModalContent>\n          <ModalHeader>Team Registration for {data.name}</ModalHeader>\n          <ModalCloseButton />\n          <ModalBody>\n            <FormControl>\n              <FormLabel>Your Shaastra ID</FormLabel>\n              <Input type=\"text\"></Input>\n            </FormControl>\n            <FormControl marginTop=\"4vh\">\n              <RadioGroup onChange={setRadio} value={radio}>\n                <Radio value=\"i\" marginRight=\"2vw\">\n                  Register as Individual\n                </Radio>\n                <Radio value=\"t\">Register as Team</Radio>\n              </RadioGroup>\n            </FormControl>\n            <FormControl marginTop=\"1.5vh\" isRequired>\n              <FormLabel>Team Name</FormLabel>\n              <Input\n                type=\"text\"\n                value={teamname}\n                onChange={(e) => setTeamname(e.target.value)}\n              ></Input>\n              {radio === \"t\" ? (\n                <React.Fragment>\n                  <FormLabel marginTop=\"1.5vh\">\n                    Team Members Shaastra IDs\n                  </FormLabel>\n                  {members.map((member, index) => {\n                    return (\n                      <React.Fragment key={index}>\n                        <Flex width={\"100%\"}>\n                          <FormControl isRequired>\n                            <Flex width=\"100%\" justifyContent=\"space-between\">\n                              <Input\n                                id={\"member\" + index}\n                                placeholder={`Team Member ${\n                                  index + 1\n                                } Shaastra ID`}\n                                required\n                                onChange={(event) =>\n                                  handleMembersInput({ index, event })\n                                }\n                              ></Input>\n                            </Flex>\n                          </FormControl>\n\n                          <Flex p={[0, 3]} flexDirection={[\"column\", \"row\"]}>\n                            {index === 0 ? null : (\n                              <Button\n                                mx={2}\n                                my={1}\n                                size={\"xs\"}\n                                onClick={() => {\n                                  const values = [...members];\n                                  values.splice(index, 1);\n                                  setMembers(values);\n                                }}\n                              >\n                                <MinusIcon />\n                              </Button>\n                            )}\n                            {index >= data.teamSize - 2 ? null : (\n                              <Button\n                                mx={2}\n                                my={1}\n                                size={\"xs\"}\n                                onClick={() => {\n                                  if (members.length < data.teamSize - 1) {\n                                    setMembers([...members, \"\"]);\n                                  }\n                                }}\n                              >\n                                <AddIcon />\n                              </Button>\n                            )}\n                          </Flex>\n                        </Flex>\n                      </React.Fragment>\n                    );\n                  })}\n                  <FormLabel>Max team size :{data.teamSize}</FormLabel>\n                </React.Fragment>\n              ) : null}\n            </FormControl>\n          </ModalBody>\n\n          <ModalFooter>\n            <Button\n              colorScheme=\"blue\"\n              mr={3}\n              onClick={() => Teamregistration(data.id)}\n            >\n              Register\n            </Button>\n          </ModalFooter>\n        </ModalContent>\n      </Modal>\n    </div>\n  );\n};\n\nexport default RegisterNow;\n","/mnt/Volume1/ShaastraWebOps/shaastra-general-website/src/components/pages/socialEndeavours/HeaderSE.tsx",["610","611"],"import { ReactComponent as LogoBlack } from \"../../../images/logo/static/Main_logo_black.svg\"\nimport { ReactComponent as LogoWhite } from \"../../../images/logo/static/Main_logo_white.svg\"\nimport { useColorModeValue } from \"@chakra-ui/color-mode\";\nimport { useBreakpointValue } from \"@chakra-ui/media-query\";\nimport { Link } from \"react-router-dom\";\nimport * as React from \"react\";\nimport { Box, Flex } from \"@chakra-ui/layout\";\nimport CustomDrawer from \"../../shared/Drawer/CustomDrawer\";\nimport { useDisclosure } from \"@chakra-ui/hooks\";\nimport { ColorModeSwitcher } from \"./ColorModeSwitcherSE\";\nimport { HamburgerIcon } from \"@chakra-ui/icons\";\n\ninterface Props {}\n\nconst Header = (props: Props) => {\n    \n    const { onOpen, isOpen, onClose } = useDisclosure();\n    \n    const drawerOpenHandler = () => {\n      onOpen();\n    };\n\n    const HeaderLogo =LogoWhite;\n    const height = useBreakpointValue({ base: \"33\", lg: \"40\" })\n  return (\n    <Flex w=\"100vw\" position=\"absolute\" p={3} flexDirection=\"row\" justifyContent=\"space-between\" alignItems=\"center\" height=\"min\">\n      <CustomDrawer onClose={onClose} isOpen={isOpen} />\n      